# MCP部署管理器环境变量配置

# 服务器配置
MCP_SERVER_NAME=mcp-deployment-manager
MCP_SERVER_VERSION=1.0.0
LOG_LEVEL=info

# Cursor配置路径 (Windows用户目录会自动检测)
CURSOR_CONFIG_PATH=

# 备份配置
ENABLE_BACKUP=true
MAX_BACKUPS=10

# 安全配置
ENABLE_AUTH=false
API_KEY=

# 网络配置
REQUEST_TIMEOUT=30000
MAX_PAYLOAD_SIZE=10485760

# 功能开关
ENABLE_RESOURCES=true
ENABLE_TOOLS=true
ENABLE_PROMPTS=true
ENABLE_SAMPLING=false 